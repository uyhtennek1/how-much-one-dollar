{
    "manifest_version": 3,
    "name": "How Much One Dollar",
    "description": "See how much is one dollar in NTD/JPY/EUR/...",
    "version": "0.1",
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": {
            "16":"./icons/icon16.png",
            "32":"./icons/icon32.png",
            "48":"./icons/icon48.png",
            "128":"./icons/icon128.png"
        }
    },
    "background": {
        "service_worker": "service-worker.js"
    },
    "permissions": [
        "alarms", "storage", "management"
    ],
    "icons": {
        "16":"./icons/icon16.png",
        "32":"./icons/icon32.png",
        "48":"./icons/icon48.png",
        "128":"./icons/icon128.png"
    }
}
